{
  "name": "@photobot/gphoto2-camera",
  "version": "1.5.0",
  "license": "MIT",
  "main": "src/camera.js",
  "types": "src/camera.d.ts",
  "author": "Matt Oakes <hello@mattoakes.net>",
  "scripts": {
    "lint": "eslint --ext .js,.ts src/",
    "test": "yarn lint",
    "install": "./scripts/checkAvailable.sh || ./scripts/downloadPrebuilt.sh || npm run build",
    "build": "./scripts/build.sh",
    "package": "npm run package:darwin:x86_64 && npm run package:darwin:arm64",
    "package:darwin:x86_64": "ARCH=x86_64 ./scripts/build.sh && ARCH=x86_64 ./scripts/package.sh",
    "package:darwin:arm64": "ARCH=arm64 ./scripts/build.sh && ARCH=arm64 ./scripts/package.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
